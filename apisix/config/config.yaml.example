plugin_attr:
  server-info:
    report_ttl: 60
  skywalking:
    report_interval: 3
    endpoint_addr: http://127.0.0.1:12800
    service_name: APISIX
    service_instance_name: APISIX Instance Name
  prometheus:
    export_addr:
      ip: 127.0.0.1
      port: 9091
    metric_prefix: apisix_
    enable_export_server: true
    export_uri: /apisix/prometheus/metrics
  opentelemetry:
    trace_id_source: x-request-id
    set_ngx_var: false
    batch_span_processor:
      max_export_batch_size: 16
      drop_on_queue_full: false
      max_queue_size: 1024
      batch_timeout: 2
      inactive_timeout: 1
    collector:
      request_headers:
        Authorization: token
      address: 127.0.0.1:4318
      request_timeout: 3
    resource:
      service.name: APISIX
  zipkin:
    set_ngx_var: false
  dubbo-proxy:
    upstream_multiplex_count: 32
  proxy-mirror:
    timeout:
      connect: 60s
      send: 60s
      read: 60s
  log-rotate:
    max_kept: 168
    enable_compression: false
    timeout: 10000
    max_size: -1
    interval: 3600
  inspect:
    hooks_file: /usr/local/apisix/plugin_inspect_hooks.lua
    delay: 3
graphql:
  max_size: 1048576
stream_plugins:
- ip-restriction
- limit-conn
- mqtt-proxy
- syslog
...
